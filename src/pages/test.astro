---
import Layout from '../layouts/Layout.astro';
import { getEntry } from 'astro:content';


const test = await getEntry('pages', 'test');
if (!test) throw new Error('Missing test content at src/content/pages/test.md');

const { title } = test.data;
// const { title = 'nhope' } = Astro.props;

---

<Layout pageTitle={"Test"}>
{title}
</Layout>